                "# %%\n",
                "# 因为这个notebook在experiment文件夹下，所以需要返回上一级目录\n",
                "FEATURE_DIR = './feature_dfs'\n",
                "files = [f for f in os.listdir(FEATURE_DIR) if f.endswith('.parquet')]\n",
                "files.sort()\n",
                "TARGET_FEATURE_FILE = files[-1]\n",
                "print(f\"将要分析的特征文件是: {TARGET_FEATURE_FILE}\")"
